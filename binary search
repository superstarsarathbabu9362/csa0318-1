# 5TH QUEESTION
# BINARY SEARCH
#incldue<stdio.h>
int main()
{
   int arr[]={1,2,3,4,5,6,78,90};
   int size=sizeof(arr)/sizeof(arr[0]);
   int key=3;
   int low,high=size;
   mid=(low+high)/2;
   if(key==arr[mid]
   {
     break;
    }
    if(key<arr[mid])
    {
      high=mid-1;
    }
    else
    {
       low=mid+1;
    }
  }
  printf("position of key %d is %d\n",key,mid);
}
